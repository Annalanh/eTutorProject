									</svg>

									<!--<i class="flaticon2-search-1"></i>-->
								</span>
							</span>
						</div>
					</form>
				</div>
			</div>
			<div class="kt-subheader__toolbar">
				<button type="button" class="btn btn-brand btn-icon-sm" data-toggle="modal"
					data-target="#et_modal_create" aria-haspopup="true" aria-expanded="false"
					id="et-create-class-btn">
					<i class="flaticon2-plus"></i> Assign
				</button>
			</div>
		</div>
	</div>
	<!-- end:: Content Head -->

	<!--Begin::Section-->
	<div class="row" id="main-container">
	</div>
	<!--End::Section-->

	<!--begin: Modal for create-->
	<div class="modal fade" id="et_modal_create" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
		aria-hidden="true">
		<div class="modal-dialog modal-lg" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLabel">Add new class</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					</button>
				</div>
				<div class="modal-body">
					<form class="kt-form">
						<div class="kt-portlet__body">
							<div class="form-group">
								<label>Class name:</label>
								<input type="text" class="form-control" placeholder="Enter class name"
									id="et-create-classname-input">
								<span class="form-text text-muted">Please enter the class name, must be
									unique</span>
							</div>
							<div class="form-group">
								<label for="tutorSelect">Select Tutor</label>
								<select class="form-control" id="et-add-tutor">
								</select>
							</div>
							{{!-- <div class="form-group">
								<label for="exampleSelect2">Example multiple select</label>
								<select multiple="" class="form-control" id="exampleSelect2">
									<option>1</option>
									<option>2</option>
									<option>3</option>
									<option>4</option>
									<option>5</option>
								</select>
							</div> --}}
						</div>
					</form>

					<!--end::Form-->
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" class="btn btn-primary" data-toggle="modal"
						data-target="#et_modal_confirm_create">Assign</button>
				</div>
			</div>
		</div>
	</div>
	<!--end: Modal for create-->

		<!--begin: Modal for edit-->
	<div class="modal fade" id="et_modal_update" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-lg" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLabel">Update Class</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					</button>
				</div>
				<div class="modal-body">
					<form class="kt-form">
						<div class="kt-portlet__body">
							<div class="form-group">
								<label>Class name:</label>
								<input type="text" class="form-control" placeholder="Enter class name" id="et-edit-classname-input">
							</div>
							<div class="form-group">
								<label for="tutorSelect">Select Tutor</label>
								<select class="form-control" id="et-update-tutor">
								</select>
							</div>
							<div class="form-group">
								<label for="tutorSelect">Select Staff</label>
								<select class="form-control" id="et-update-staff">
								</select>
							</div>
						</div>
					</form>

					<!--end::Form-->
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#et_modal_confirm_update">Update class</button>
				</div>
			</div>
		</div>
	</div>
	<!--end: Modal for edit-->
	

	<!--begin: Modal for create confirmation -->
	<div class="modal fade" id="et_modal_confirm_create" tabindex="-1" role="dialog"
		aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
		<div class="modal-dialog modal-dialog-centered" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLongTitle">Confirm!</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					</button>
				</div>
				<div class="modal-body">
					<p>Do you want to create this class ?</p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" class="btn btn-primary" id="et-create-class-confirm-btn">Create</button>
				</div>
			</div>
		</div>
	</div>
	<!--end: Modal for add confirmation -->


		<!--begin: Modal for delete confirmation -->
	<div class="modal fade" id="et_modal_delete" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
		<div class="modal-dialog modal-dialog-centered" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLongTitle">Confirm!</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					</button>
				</div>
				<div class="modal-body">
					<p>Do you want to delete this class ?</p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" class="btn btn-danger" id="et_modal_confirm_delete">Delete</button>
				</div>
			</div>
		</div>
	</div>
	<!--end: Modal for delete confirmation -->

	<!--begin: Modal for update confirmation -->
	<div class="modal fade" id="et_modal_confirm_update" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
		<div class="modal-dialog modal-dialog-centered" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLongTitle">Confirm!</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					</button>
				</div>
				<div class="modal-body">
					<p>Do you want to update this class ?</p>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" class="btn btn-primary" id="et_modal_confirm_update" >Update</button>
				</div>
			</div>
		</div>
	</div>
	<!--end: Modal for update confirmation -->

	<!-- Error Modal -->
	<div class="modal" id="errorModal">
		<div class="modal-dialog">
			<div class="modal-content">

				<!-- Modal Header -->
				<div class="modal-header">
					<h4 class="modal-title"><span id="error-title">Error</span></h4>
					<button type="button" class="close" data-dismiss="modal">&times;</button>
				</div>

				<!-- Modal body -->
				<div class="modal-body">
					<span id="error-body">Something went wrong!</span>
				</div>

				<!-- Modal footer -->
				<div class="modal-footer">
					<button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
				</div>

			</div>
		</div>
	</div>


	<!--Begin::Section-->
	<div class="row" id="main-container">
	</div>
	<!--End::Section-->

	

</div>
